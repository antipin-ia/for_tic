{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(?!dist/).*",
        "dest": "/dist/$0",
        "headers": { "cache-control": "public,max-age=0,must-revalidate" }
      }
    ]
  }